{
  "name": "smali-hover-help",
  "publisher": "gGaster",
  "displayName": "Smali Hover Help",
  "description": "Hover descriptions for Smali operators",
  "version": "1.1.0",
  "engines": {
      "vscode": "^1.75.0"
  },
  "categories": [
      "Programming Languages"
  ],
  "activationEvents": [
      "onLanguage:smali"
  ],
  "contributes": {
      "languages": [
          {
              "id": "smali",
              "aliases": ["Smali", "smali"],
              "extensions": [".smali"]
          }
      ]
  },
  "main": "./out/extension.js",
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
      "@types/vscode": "^1.75.0",
      "typescript": "^4.9.4"
  }
  
}